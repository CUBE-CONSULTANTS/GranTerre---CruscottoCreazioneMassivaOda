<c:FragmentDefinition
	xmlns="sap.m"
	xmlns:c="sap.ui.core"
>
<Dialog title="{i18n>ListaErrori}" stretch="true">
<Table
	
	items="{errorUploadModel>/results}"
>
	<columns>
        <Column>
            <Text text="{i18n>Riga}"/>
        </Column>
        <Column>
            <Text text="{i18n>Tipo}"/>
        </Column>
        <Column>
            <Text text="{i18n>MessaggioError}"/>
        </Column>
        <Column>
            <Text text="{i18n>Campo}"/>
        </Column>
        <Column>
            <Text text="{i18n>Parte}"/>
        </Column>
         <Column>
            <Text text="{i18n>Indicatore}"/>
        </Column>
        <Column>
            <Text text="{i18n>Progressivo}"/>
        </Column>
    </columns>
    <items>
        <ColumnListItem>
            <cells>
                <Text text="{errorUploadModel>LineItem}"/>
                <Text text="{errorUploadModel>Type}"/>
                <Text text="{errorUploadModel>ZzerrorMsg}"/>
                <Text text="{errorUploadModel>Zzfieldlabel}"/>
                <Text text="{errorUploadModel>Zzpart}"/>
                <Text text="{errorUploadModel>Zzindicator}"/>
                <Text text="{errorUploadModel>Zzprogressive}"/>
            </cells>
        </ColumnListItem>
    </items>
</Table>
<buttons>
<Button type="Emphasized" press="onClose" icon="sap-icon://decline" text="{i18n>Chiudi}"/>
</buttons>
</Dialog>
</c:FragmentDefinition>